<!DOCTYPE html>
<html>
<head>
  <title>Music</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if logged_in? %>
	<p>Logged in as <%= current_user.email %>.</p>
	<%= link_to "Log out", session_url, :method => :delete %>
<% end %>
<%= yield %>

</body>
</html>
